#include<stdio.h>
#include<conio.h>
int main()
{
	int a;
	printf("Enter your mark:",a);
	scanf("%d",&a);
	if(a>=85 && a<=100)
	{
		printf("Grade A");
	}
	else if(a>=70 && a<=84)
	{
		printf("Grae B");
	}
	else if(a>=55 && a<=69)
	{
		printf("Grade C");
	}
	else if(a>=40 && a<=54)
	{
		printf("Grade D");
	}
	else
	{
		printf("Fail");
	}
	return 0;   
}
